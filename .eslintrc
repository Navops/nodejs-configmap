{
  "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 8,
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true
      }
  },
  "rules": {
      "no-console": 1,
      "indent": [2,2],
      "quotes": [
          2,
          "single"
      ],
      "linebreak-style": [
          2,
          "unix"
      ],
      "semi": [
          2,
          "never"
      ]
  },
  "env": {
      "es6": true,
      "node": true
  },
  "extends": "eslint:recommended"
}
